{
  "name": "@dxos/feed-level-indexer",
  "version": "1.0.0-beta.5",
  "description": "Index messages into a leveldb through multiple hypercore feeds.",
  "homepage": "https://github.com/dxos/feed-level-indexer#readme",
  "bugs": {
    "url": "https://github.com/dxos/feed-level-indexer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/feed-level-indexer.git"
  },
  "license": "GPL-3.0",
  "files": [
    "dist",
    "index.js"
  ],
  "main": "index.js",
  "browser": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:babel",
    "build:babel": "babel ./src --out-dir ./dist --ignore \"**/*.test.js\" --source-maps",
    "clean": "del-cli dist",
    "coverage": "npm test -- --coverage",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "semistandard 'src/**/*.js'",
    "prepublishOnly": "npm run build && npm run test",
    "test": "jest --verbose --passWithNoTests src",
    "posttest": "npm run lint"
  },
  "browserslist": [
    "> 5%"
  ],
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "end-of-stream": "^1.4.4",
    "level": "^6.0.0",
    "level-live": "github:Frando/level-live#key-encodings",
    "nanoresource": "^1.3.0",
    "pump": "^3.0.0",
    "reachdown": "^1.0.0",
    "source-map-support": "^0.5.16",
    "subleveldown": "^4.1.4",
    "through2": "^3.0.1",
    "varint": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@dxos/feed-store": "^1.2.0-beta.13",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-inline-import": "^3.0.0",
    "coveralls": "^3.0.9",
    "cpy-cli": "^3.1.0",
    "del-cli": "^3.0.0",
    "hypercore-crypto": "^1.0.0",
    "jest": "^25.1.0",
    "level-mem": "^5.0.1",
    "pify": "^5.0.0",
    "random-access-memory": "^3.1.1",
    "semistandard": "^14.2.0",
    "tempy": "^0.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "env": [
      "jest",
      "node",
      "browser"
    ]
  }
}
